{
  "name": "quillo_frontend",
  "version": "0.2.0",
  "description": "Quillo",
  "type": "module",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "dev": "cd ./src/quillo_frontend && vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prebuild": "dfx generate",
    "start": "cd ./src/quillo_frontend && npm run dev",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate quillo_backend",
    "gen-deploy": "./did.sh && sudo dfx generate && sudo dfx deploy -y"
  },
  "dependencies": {
    "@dfinity/agent": "^0.20.2",
    "@dfinity/auth-client": "^0.20.2",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.1",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.9.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "events": "3.3.0",
    "html-webpack-plugin": "5.5.0",
    "process": "0.11.10",
    "sass-loader": "^14.0.0",
    "stream-browserify": "3.0.0",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "util": "0.12.4",
    "vite": "^5.0.8",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || >=17",
    "npm": "^7.17 || >=8"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
